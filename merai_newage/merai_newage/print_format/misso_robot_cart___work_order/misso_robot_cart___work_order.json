 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field -%}\n{% set data = json.loads(doc.custom_operation_form_data or \"{}\") %}\n{% if letter_head and not no_letterhead -%}\n{% if print_settings.repeat_header_footer %}\n<div class=\"letter-head\">{{ letter_head }}</div>\n{%- endif %}{%- endif %}\n\n<main style=\"font-size: 12px;\">\n<style>\n    th {\n        text-align: center;\n    }\n    table td.details-col, table th.details-col {\n        text-align: left;\n    }\n</style>\n\n<!-- Header Section -->\n<table width=\"100%\" border=\"1\" cellpadding=\"5\" cellspacing=\"0\" style=\"font-size: 12px; \">\n    <tr>\n        <th class=\"details-col\" width=\"25%\" ><b> Lot No.</b> </th>\n        <td width=\"25%\" >{{ doc.custom_batch or '' }}</td>\n        <th class=\"details-col\" colspan=\"2\" style=\"text-align: center\" ><b> Section: 01</b> </th>\n    </tr>\n    <tr>\n        <th class=\"details-col\" width=\"25%\" ><b>Product Name </b> </th>\n        <td width=\"25%\" >{{ doc.item_name or '' }}</td>\n        <th class=\"details-col\" width=\"25%\" ><b>Part No. </b> </th>\n        <td width=\"25%\" >{{ doc.production_item or '' }}</td>\n    </tr>\n    <tr>\n        <th class=\"details-col\" width=\"25%\" ><b>Lot Start date </b> </th>\n        <td width=\"25%\" >{{ frappe.utils.format_date(doc.actual_start_date, \"dd-MM-yyyy\") }}</td>\n        <th class=\"details-col\" width=\"25%\" ><b>Lot End date </b> </th>\n        <td width=\"25%\" >{{ frappe.utils.format_date(doc.actual_end_date, \"dd-MM-yyyy\") }}</td>\n    </tr>\n    <tr>\n        <th class=\"details-col\" width=\"25%\" ><b>Lot Size </b> </th>\n        <td width=\"25%\" >{{ doc.qty or '' }}</td>\n        <th class=\"details-col\" width=\"25%\" ><b>Lot Qty. Released </b> </th>\n        <td width=\"25%\" >{{ doc.produced_qty or '' }}</td>\n    </tr>\n    <tr>\n        <th class=\"details-col\" width=\"25%\" ><b>Drawing No. </b> </th>\n        <td width=\"25%\" ><
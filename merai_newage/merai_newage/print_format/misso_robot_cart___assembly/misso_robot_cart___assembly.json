 "html": "{%- from \"templates/print_formats/standard_macros.html\" import add_header, render_field -%}\n{% set data = json.loads(doc.custom_operation_form_data or \"{}\") %}\n\n<main style=\"font-size: 12px;\">\n<style>\n    th {\n        text-align: center;\n    }\n    table td.details-col, table th.details-col {\n        text-align: left;\n    }\n</style>\n\n\n<p><b>[2] Assembly</b></p>\n<br>\n\n<table border=\"1\" width=\"100%\" style=\"font-size: 12px; text-align: center; border-collapse: collapse;\">\n    <tr>\n        <th>Sr. No.</th>\n        <th>Stage</th>\n        <th>Line Clearance Criteria</th>\n        <th>Yes/No</th>\n        <th>Sign/Date</th>\n    </tr>\n    {% if doc.custom_line_clearance_checklist %}\n        {% for row in doc.custom_line_clearance_checklist %}\n            <tr>\n                {% if loop.first %}\n                    <td rowspan=\"{{ doc.custom_line_clearance_checklist | length }}\">1</td>\n                    <td rowspan=\"{{ doc.custom_line_clearance_checklist | length }}\">{{ doc.operation or '' }}</td>\n                {% endif %}\n                <td style=\"text-align: left; padding: 4px;\">{{ row.line_clearance_criteria or '' }}</td>\n                <td>{{ row.yesno or '' }}</td>\n                {% if loop.first %}\n                    <td rowspan=\"{{ doc.custom_line_clearance_checklist | length }}\"></td>\n                {% endif %}\n            </tr>\n        {% endfor %}\n    {% else %}\n        <tr>\n            <td>1</td>\n            <td>{{ doc.operation or '' }}</td>\n            <td colspan=\"3\">No criteria defined</td>\n        </tr>\n    {% endif %}\n</table>\n\n\n<br>\n<p><b>Remarks:</b>______________________________________</p>\n<br>\n<p><b>(2.1) Sub Assembly:</b></p>\n<br>\n<p><b>(A) Sub Assembly performed by production:</b></p>\n\n<table border=\"1\" width=\"100%\" style=\"font-size: 12px; text-align: center;\">\n    <tr>\n        <th>Sr. No.</th>\n        <th>Part No</th>\n        <th>Hardware Component